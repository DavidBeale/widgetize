{
  "name": "example-widget",
  "main": "ExampleWidget.js",
  "browserify": {
    "transform": [
      "html2js-browserify",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "add-module-exports"
          ]
        }
      ]
    ]
  },
  "style": "*.less",
  "transforms": [
    "less-css-stream"
  ],
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "html2js-browserify": "^1.0.0",
    "less-css-stream": "^1.0.0"
  }
}
